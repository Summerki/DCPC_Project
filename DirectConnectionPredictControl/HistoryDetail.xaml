<Window x:Class="DirectConnectionPredictControl.HistoryDetail"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:MYUI="clr-namespace:DirectConnectionPredictControl"
        xmlns:local="clr-namespace:DirectConnectionPredictControl"
        mc:Ignorable="d"
        Name="historyDetail"
        Title="历史数据详情" Height="900" Width="1400"
        Style="{StaticResource NormalWinStyle}" Closed="historyDetail_Closed">
    <Window.Resources>

        <Storyboard x:Key="open">
            <DoubleAnimation From="0" To="1" Duration="0:0:0.5"
                                 Storyboard.TargetName="historyDetail"
                                 Storyboard.TargetProperty="(UIElement.OpacityMask).(GradientBrush.GradientStops)[0].Offset"/>
            <DoubleAnimation From="0" To="1" BeginTime="0:0:0.1" Duration="0:0:0.4"
                                 Storyboard.TargetName="historyDetail"
                                 Storyboard.TargetProperty="(UIElement.OpacityMask).(GradientBrush.GradientStops)[1].Offset"/>
            <ColorAnimation BeginTime="0:0:0.5" Duration="0:0:0" From="Transparent" To="Yellow" Storyboard.TargetName="historyDetail"
                                 Storyboard.TargetProperty="(UIElement.OpacityMask).(GradientBrush.GradientStops)[0].Color" />
        </Storyboard>
        <Storyboard x:Key="close"  Completed="Storyboard_Completed">
            <DoubleAnimation From="1" To="0" Duration="0:0:0.4"
                                 Storyboard.TargetName="historyDetail"
                                 Storyboard.TargetProperty="(UIElement.OpacityMask).(GradientBrush.GradientStops)[0].Offset"/>
            <DoubleAnimation Duration="0:0:.3" BeginTime="0:0:0.1" From="1" To="0"
                                 Storyboard.TargetName="historyDetail"
                                 Storyboard.TargetProperty="(UIElement.OpacityMask).(GradientBrush.GradientStops)[1].Offset"/>
            <ColorAnimation  Duration="0" From="Yellow" To="Transparent"
                                Storyboard.TargetName="historyDetail"
                                 Storyboard.TargetProperty="(UIElement.OpacityMask).(GradientBrush.GradientStops)[1].Color"/>
        </Storyboard>
    </Window.Resources>



    <!--透明掩码-->
    <Window.OpacityMask>
        <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
            <GradientStop Offset="0" Color="Transparent"></GradientStop>
            <GradientStop Offset="0" Color="Yellow"></GradientStop>
        </LinearGradientBrush>
    </Window.OpacityMask>

    <Window.Triggers>
        <EventTrigger RoutedEvent="Button.Click">
            <BeginStoryboard Storyboard="{StaticResource close}"></BeginStoryboard>
            <EventTrigger.SourceName>closeBtn</EventTrigger.SourceName>
        </EventTrigger>
    </Window.Triggers>
    <Window.BorderBrush>
        <LinearGradientBrush EndPoint="1,0" StartPoint="0,0">
            <GradientStop Color="#36BAFE" Offset="1"/>
            <GradientStop Color="#4E8DFE" Offset="0"/>
        </LinearGradientBrush>
    </Window.BorderBrush>
    <DockPanel>
        <Grid DockPanel.Dock="Top">

            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"/>



                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>

            <Grid Grid.Row="0" MouseLeftButtonDown="Grid_MouseLeftButtonDown" >
                <Grid.Background>
                    <LinearGradientBrush EndPoint="1,0" StartPoint="0,0">
                        <GradientStop Color="#36BAFE" Offset="1"/>
                        <GradientStop Color="#3399ff" Offset="0"/>
                    </LinearGradientBrush>
                </Grid.Background>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <StackPanel Grid.Column="0" Orientation="Horizontal"  HorizontalAlignment="Left" >
                    <Image Source="Asserts/Image/speed_test_120px_1118805_easyicon.net.ico" Margin="8" Height="20"></Image>
                    <Label FontWeight="Normal"  VerticalContentAlignment="Center" FontSize="16"  Foreground="White" >直通预控维护软件</Label>
                </StackPanel>
                <StackPanel Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Right">
                    <Button Name="miniumBtn" Content="-"  Click="miniumBtn_Click" VerticalAlignment="Center" Style="{StaticResource MinMaxButtonStyle}"/>
                    <Button Name="maximunBtn" Content="□" Click="maximunBtn_Click"  HorizontalContentAlignment="Center"  VerticalAlignment="Center" Style="{StaticResource MinMaxButtonStyle}"/>
                    <Button Name="closeBtn"  Content="×" VerticalAlignment="Center" Style="{StaticResource CloseButtonStyle}" Click="closeBtn_Click"/>
                </StackPanel>
            </Grid>
            <Menu Grid.Row="1">
                <Menu.Background>
                    <LinearGradientBrush EndPoint="1,0" StartPoint="0,0">
                        <GradientStop Color="#36BAFE" Offset="1"/>
                        <GradientStop Color="#3399ff" Offset="0"/>
                    </LinearGradientBrush>
                </Menu.Background>
                <MenuItem Header="窗口配置" Style="{StaticResource MenuItemStyle}">
                    <MenuItem Name="columnConfigItem" Header="列显示配置" Click="columnConfigItem_Click" Style="{StaticResource SubMenuItemStyle}"></MenuItem>
                </MenuItem>
            </Menu>
            <ToolBar Grid.Row="2" Background="#EBEEF2" Loaded="ToolBar_Loaded">
                <Button VerticalAlignment="Center" Name="preTool" Click="preEvent" Style="{StaticResource ToolBarButtonStyle}" Width="Auto"  Height="Auto">
                    <Image Width="18" Source="Asserts/Image/forward.ico"></Image>
                </Button>
                <Button VerticalAlignment="Center" Name="aftTool" Click="aftEvent" Style="{StaticResource ToolBarButtonStyle}" Width="Auto"   Height="Auto">
                    <Image Width="18" Source="Asserts/Image/after.ico"></Image>
                </Button>
                <CheckBox Name="showAnalog" IsChecked="True" Margin="15,0,0,0" Checked="showAnalog_Checked" Unchecked="showAnalog_Unchecked">模拟量</CheckBox>
                <CheckBox Name="showDigital" IsChecked="True" Margin="5,0,0,0">数字量</CheckBox>


                <DatePicker Margin="5,2,0,0"  Name="DateChoose" Focusable="False" ></DatePicker>
                <!--现在先不加Focusable="False"，为了测试-->
                <!--<MYUI:ComboBoxControl x:Name="HourSelect" ></MYUI:ComboBoxControl>-->
                <ComboBox Height="27.2" Margin="5,2,0,0" Width="40" Name="HourComboBox" Focusable="False"></ComboBox>
                <TextBlock HorizontalAlignment="Center"  VerticalAlignment="Center">时</TextBlock>
                <ComboBox Height="27.2" Margin="5,2,0,0" Width="40" Name="MinuteComboBox" Focusable="False" />
                <TextBlock HorizontalAlignment="Center"  VerticalAlignment="Center">分</TextBlock>
                <ComboBox Height="27.2" Margin="5,2,0,0" Width="40" Name="SecondComboBox" Focusable="False"/>
                <TextBlock HorizontalAlignment="Center"  VerticalAlignment="Center">秒</TextBlock>
                <Button Margin="5,0,0,0" Content="搜索" Background="#b0d2f5" Name="HistoryDeatilSearchBtn" Click="HistoryDeatilSearchBtn_Click"></Button>

                <TextBox Margin="10,0,0,0" Width="100" Name="SearchTextBox" ></TextBox>
                <Button Margin="5,0,0,0" Content="测试搜索" Background="#b0d2f5" Name="IDSearch" Click="IDSearch_Click" ></Button>
                <ComboBox Height="27.2"  Width="auto" Name="SearchResultComboBox" Focusable="False" HorizontalAlignment="Right" />
                <Button Margin="5,0,0,0" Content="跳转" Background="#b0d2f5" Name="JumpTo"></Button>
                <Button Margin="5,0,0,0" Content="生成JSON文件" Background="#b0d2f5" Name="JSONBtn" Click="JSONBtn_Click"></Button>
            </ToolBar>
        </Grid>
        <Grid DockPanel.Dock="Bottom">
            <Grid.Background>
                <LinearGradientBrush EndPoint="1,0" StartPoint="0,0">
                    <GradientStop Color="#36BAFE" Offset="1"/>
                    <GradientStop Color="#3399ff" Offset="0"/>
                </LinearGradientBrush>
            </Grid.Background>
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <StackPanel Orientation="Horizontal" Grid.Column="0">
                <Label Foreground="White">已读取的行数:</Label>
                <Label Name="lineHasReadLbl" Content="{Binding ElementName=totalPageLbl, Path=Content, Mode=OneWay}" Margin="5,0,0,0" Foreground="Orange"></Label>
                <Label Foreground="White" Margin="50,0,0,0">总字节量:</Label>
                <Label Name="byteHasReadLbl" Margin="5,0,0,0" Foreground="Orange"></Label>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Grid.Column="1" HorizontalAlignment="Center">
                <Button Name="preBtn" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Click="preEvent">←</Button>
                <Label Name="nowPageLbl" Foreground="White">1</Label>
                <Label Foreground="White">/</Label>
                <Label Name="totalPageLbl" Foreground="White">200</Label>
                <Button Name="aftBtn" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Click="aftEvent">→</Button>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Grid.Column="2" HorizontalAlignment="Right">
                <Label Foreground="White">当前状态:</Label>
                <Label Name="stateLbl" Margin="5,0,0,0" Foreground="Orange">就绪</Label>
            </StackPanel>
        </Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
            </Grid.RowDefinitions>
            <DataGrid Name="historyList" Grid.Row="0" AutoGenerateColumns="False" CanUserAddRows="False" >
