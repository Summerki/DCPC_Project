<UserControl x:Class="DirectConnectionPredictControl.ComboBoxControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:MYUI="clr-namespace:DirectConnectionPredictControl"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             mc:Ignorable="d">

    <Grid>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <MYUI:NewComboBox x:Name="comboBox" Width="150" Height="25" DisplayMemberPath="Number"
                           SelectedValuePath="ID" IsEditable="True" >
               
            </MYUI:NewComboBox>
          
        </StackPanel>
    </Grid>
</UserControl>
