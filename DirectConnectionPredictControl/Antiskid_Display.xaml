                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="80"></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition Width="80"></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Label Foreground="DodgerBlue" Grid.Column="0">项目</Label>
                            <Label Foreground="DodgerBlue" Grid.Column="1">值</Label>
                            <Label Foreground="DodgerBlue" Grid.Column="2">项目</Label>
                            <Label Foreground="DodgerBlue" Grid.Column="3">值</Label>
                        </Grid>
                        <ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Auto">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="80"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition Width="80"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Label Grid.Row="0" Grid.Column="0">架1减速度</Label>
                                <Label Grid.Row="1" Grid.Column="0">架3减速度</Label>
                                <Label Grid.Row="2" Grid.Column="0">架5减速度</Label>
                                <Label Grid.Row="3" Grid.Column="0">架1滑移率</Label>
                                <Label Grid.Row="4" Grid.Column="0">架3滑移率</Label>
                                <Label Grid.Row="5" Grid.Column="0">架5滑移率</Label>
                                <Label Grid.Row="6" Grid.Column="0">架1滑行等级</Label>
                                <Label Grid.Row="7" Grid.Column="0">架3滑行等级</Label>
                                <Label Grid.Row="8" Grid.Column="0">架5滑行等级</Label>

                                <Label Grid.Row="0" Grid.Column="2">架2减速度</Label>
                                <Label Grid.Row="1" Grid.Column="2">架4减速度</Label>
                                <Label Grid.Row="2" Grid.Column="2">架6减速度</Label>
                                <Label Grid.Row="3" Grid.Column="2">架2滑移率</Label>
                                <Label Grid.Row="4" Grid.Column="2">架4滑移率</Label>
                                <Label Grid.Row="5" Grid.Column="2">架6滑移率</Label>
                                <Label Grid.Row="6" Grid.Column="2">架2滑行等级</Label>
                                <Label Grid.Row="7" Grid.Column="2">架4滑行等级</Label>
                                <Label Grid.Row="8" Grid.Column="2">架6滑行等级</Label>
                                
                                <!--减速度-->
                                <Label Grid.Row="0" Grid.Column="1" Name="row_0_column_1_wsp"></Label>
                                <Label Grid.Row="0" Grid.Column="3" Name="row_0_column_3_wsp"></Label>
                                <Label Grid.Row="1" Grid.Column="1" Name="row_1_column_1_wsp"></Label>
                                <Label Grid.Row="1" Grid.Column="3" Name="row_1_column_3_wsp"></Label>
                                <Label Grid.Row="2" Grid.Column="1" Name="row_2_column_1_wsp"></Label>
                                <Label Grid.Row="2" Grid.Column="3" Name="row_2_column_3_wsp"></Label>
                                
                                <!--滑行率-->
                                <Label Grid.Row="3" Grid.Column="1" Name="row_3_column_1_wsp"></Label>
                                <Label Grid.Row="3" Grid.Column="3" Name="row_3_column_3_wsp"></Label>
                                <Label Grid.Row="4" Grid.Column="1" Name="row_4_column_1_wsp"></Label>
                                <Label Grid.Row="4" Grid.Column="3" Name="row_4_column_3_wsp"></Label>
                                <Label Grid.Row="5" Grid.Column="1" Name="row_5_column_1_wsp"></Label>
                                <Label Grid.Row="5" Grid.Column="3" Name="row_5_column_3_wsp"></Label>
                                
                                <!--滑行等级-->
                                <Label Grid.Row="6" Grid.Column="1" Name="row_6_column_1_wsp"></Label>
                                <Label Grid.Row="6" Grid.Column="3" Name="row_6_column_3_wsp"></Label>
                                <Label Grid.Row="7" Grid.Column="1" Name="row_7_column_1_wsp"></Label>
                                <Label Grid.Row="7" Grid.Column="3" Name="row_7_column_3_wsp"></Label>
                                <Label Grid.Row="8" Grid.Column="1" Name="row_8_column_1_wsp"></Label>
                                <Label Grid.Row="8" Grid.Column="3" Name="row_8_column_3_wsp"></Label>
                            </Grid>
                        </ScrollViewer>
                    </Grid>
                </Grid>

                <Grid Grid.Row="2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="3.5*"></ColumnDefinition>
                        <ColumnDefinition></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <!--制动缸压力值曲线图-->
                    <Grid Grid.Column="0">
                        <d3:ChartPlotter Grid.Row="2" Name="Antiskid_pressureChart" BorderBrush="{x:Null}">
                            <d3:ChartPlotter.HorizontalAxis>
                                <d3:HorizontalAxis>

                                </d3:HorizontalAxis>
                            </d3:ChartPlotter.HorizontalAxis>
                            <d3:ChartPlotter.VerticalAxis>
                                <d3:VerticalAxis></d3:VerticalAxis>
                            </d3:ChartPlotter.VerticalAxis>
                            <d3:VerticalAxisTitle Content="压力/kpa"></d3:VerticalAxisTitle>
                            <d3:HorizontalAxisTitle Content="运行时间/s"></d3:HorizontalAxisTitle>
                        </d3:ChartPlotter>
                    </Grid>
                    <!--制动缸压力数值-->
                    <Grid Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="80"></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition Width="80"></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Label Foreground="DodgerBlue" Grid.Column="0">项目</Label>
                            <Label Foreground="DodgerBlue" Grid.Column="1">值</Label>
                            <Label Foreground="DodgerBlue" Grid.Column="2">项目</Label>
                            <Label Foreground="DodgerBlue" Grid.Column="3">值</Label>
                        </Grid>
                        <ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Auto">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="80"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition Width="80"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>

                                </Grid.RowDefinitions>
                                <Label Grid.Row="0" Grid.Column="0">架1制动缸1</Label>
                                <Label Grid.Row="1" Grid.Column="0">架2制动缸1</Label>
                                <Label Grid.Row="2" Grid.Column="0">架3制动缸1</Label>
                                <Label Grid.Row="3" Grid.Column="0">架4制动缸1</Label>
                                <Label Grid.Row="4" Grid.Column="0">架5制动缸1</Label>
                                <Label Grid.Row="5" Grid.Column="0">架6制动缸1</Label>

                                <Label Grid.Row="0" Grid.Column="2">架1制动缸2</Label>
                                <Label Grid.Row="1" Grid.Column="2">架2制动缸2</Label>
                                <Label Grid.Row="2" Grid.Column="2">架3制动缸2</Label>
                                <Label Grid.Row="3" Grid.Column="2">架4制动缸2</Label>
                                <Label Grid.Row="4" Grid.Column="2">架5制动缸2</Label>
                                <Label Grid.Row="5" Grid.Column="2">架6制动缸2</Label>

                                <Label Grid.Row="0" Grid.Column="1" Name="row_0_column_0_Pr">0.00</Label>
                                <Label Grid.Row="1" Grid.Column="1" Name="row_1_column_0_Pr">0.00</Label>
                                <Label Grid.Row="2" Grid.Column="1" Name="row_2_column_0_Pr">0.00</Label>
                                <Label Grid.Row="3" Grid.Column="1" Name="row_3_column_0_Pr">0.00</Label>
                                <Label Grid.Row="4" Grid.Column="1" Name="row_4_column_0_Pr">0.00</Label>
                                <Label Grid.Row="5" Grid.Column="1" Name="row_5_column_0_Pr">0.00</Label>

                                <Label Grid.Row="0" Grid.Column="3" Name="row_0_column_3_Pr">0.00</Label>
                                <Label Grid.Row="1" Grid.Column="3" Name="row_1_column_3_Pr">0.00</Label>
                                <Label Grid.Row="2" Grid.Column="3" Name="row_2_column_3_Pr">0.00</Label>
                                <Label Grid.Row="3" Grid.Column="3" Name="row_3_column_3_Pr">0.00</Label>
                                <Label Grid.Row="4" Grid.Column="3" Name="row_4_column_3_Pr">0.00</Label>
                                <Label Grid.Row="5" Grid.Column="3" Name="row_5_column_3_Pr">0.00</Label>


                            </Grid>
                        </ScrollViewer>
                    </Grid>
                </Grid>

                <ScrollViewer Grid.Row="3" VerticalScrollBarVisibility="Auto">
                    <Grid Grid.Row="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="130"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition Width="130"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>

                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>

                        <Label Grid.Row="0" Grid.Column="0" Foreground="DarkOrange">名称</Label>
                        <Label Grid.Row="0" Grid.Column="1" Foreground="DarkOrange">1架</Label>
                        <Label Grid.Row="0" Grid.Column="2" Foreground="DarkOrange">2架</Label>
                        <Label Grid.Row="0" Grid.Column="3" Foreground="DarkOrange">3架</Label>
                        <Label Grid.Row="0" Grid.Column="4" Foreground="DarkOrange">4架</Label>
                        <Label Grid.Row="0" Grid.Column="5" Foreground="DarkOrange">5架</Label>
                        <Label Grid.Row="0" Grid.Column="6" Foreground="DarkOrange">6架</Label>

                        <Label Grid.Row="0" Grid.Column="7" Foreground="DarkOrange">名称</Label>
                        <Label Grid.Row="0" Grid.Column="8" Foreground="DarkOrange">1架</Label>
                        <Label Grid.Row="0" Grid.Column="9" Foreground="DarkOrange">2架</Label>
                        <Label Grid.Row="0" Grid.Column="10" Foreground="DarkOrange">3架</Label>
                        <Label Grid.Row="0" Grid.Column="11" Foreground="DarkOrange">4架</Label>
                        <Label Grid.Row="0" Grid.Column="12" Foreground="DarkOrange">5架</Label>
                        <Label Grid.Row="0" Grid.Column="13" Foreground="DarkOrange">6架</Label>

                        <Label Grid.Column="0" Grid.Row="1">轴1WSP故障</Label>
                        <Label Grid.Column="0" Grid.Row="2">轴2WSP故障</Label>
                        <Label Grid.Column="0" Grid.Row="3">速度传感器1故障</Label>
                        <Label Grid.Column="0" Grid.Row="4">速度传感器2故障</Label>

                        <Label Grid.Column="7" Grid.Row="1">DCU1电制动OK</Label>
                        <Label Grid.Column="7" Grid.Row="2">DCU1电制动准备淡出</Label>
                        <Label Grid.Column="7" Grid.Row="3">DCU1电制动滑行</Label>
                        <Label Grid.Column="7" Grid.Row="4">DCU2电制动OK</Label>
                        <Label Grid.Column="7" Grid.Row="5">DCU2电制动准备淡出</Label>
                        <Label Grid.Column="7" Grid.Row="6">DCU2电制动滑行</Label>
                        <Label Grid.Column="7" Grid.Row="7">DCU3电制动OK</Label>
